<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Telephone" table="telephone">
    <indexes>
      <index name="fk_telephone_users_idx" columns="users_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="id_UNIQUE" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id"/>
    <id name="users" association-key="true"/>
    <field name="tel" type="string" column="tel" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="users" target-entity="Users" inversed-by="telephone" fetch="LAZY">
        <join-column name="users_id" referenced-column-name="id" />
    </many-to-one>
<!--    <one-to-one field="users" target-entity="Users" fetch="LAZY">
      <join-columns>
        <join-column name="users_id" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>-->
  </entity>
</doctrine-mapping>
